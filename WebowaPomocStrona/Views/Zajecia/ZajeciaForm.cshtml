@model WebowaPomocStrona.ViewModels.EditZajeciaViewModel
@{ 
    ViewBag.Title = "ZajeciaForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Dodaj Zajęcia</h2>
@using (Html.BeginForm("Save", "Zajecia"))
{
    <div class="from-group">
        @Html.LabelFor(m => m.zajecia.Nazwa)
        @Html.TextBoxFor(m => m.zajecia.Nazwa, new { @class = "form-control" })
    </div>
    @Html.HiddenFor(m => m.zajecia.Id)
    <button type="submit" class="btn btn-primary">Dodaj</button>
}


